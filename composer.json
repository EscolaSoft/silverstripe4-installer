{
    "name": "qunabu/silverstripe4-installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": ">=0.1@dev <1.0",
        "silverstripe/recipe-cms": "2.x-dev",
        "silverstripe-themes/simple": "~3.2.0",
        "qunabu/silverstripe-theme":"dev-master",
        "qunabu/silverstripe-qunabu-helpers":"4.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "assets/*",
            "mysite/_config/*",
            ".env.example",
            "favicon.ico"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    
    "minimum-stability": "dev",
    "scripts": {
        "post-create-project-cmd": [
          "php post-install.php"
        ]
  }
}
