{
    "name": "qunabu/silverstripe4-installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "1.x-dev",
        "silverstripe/recipe-cms": "4.4.1",
        "silverstripe-themes/simple": "~3.2.0",
        "silverstripe/elemental-blocks": "dev-master",
        "qunabu/silverstripe-theme": "dev-master",
        "qunabu/silverstripe-qunabu-helpers": "4.0.x-dev",
        "sheadawson/silverstripe-linkable": "^2.0@dev",
        "magnum34/silverstripe-svg-go": "dev-master",
        "silverstripe/blog": "^4.0@dev",
        "tractorcow/silverstripe-fluent": "dev-master",
        "dnadesign/silverstripe-elemental-virtual": "^1.0@dev",
        "dnadesign/silverstripe-elemental-list": "^1.0@dev",
        "silverstripe/tagfield": "^2.0@dev",
        "edgarindustries/youtubefield": "dev-master",
        "undefinedoffset/sortablegridfield": "^2.0@dev",
        "bummzack/sortablefile": "^2.0"


    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "assets/*",
            "mysite/_config/*",
            ".env.example",
            "favicon.ico"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-create-project-cmd": [
            "php post-install.php"
        ]
    }
}
